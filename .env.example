# Copy this file to .env.local and fill in your values.
# .env.local is gitignored — your URLs will never be committed.

# Google Drive resume link (Share → Copy link; use the full URL)
NEXT_PUBLIC_RESUME_URL=https://drive.google.com/file/d/YOUR_FILE_ID/view

# Formspree form endpoint (from formspree.io dashboard)
NEXT_PUBLIC_FORMSPREE_ENDPOINT=https://formspree.io/f/YOUR_FORM_ID

# Optional: GitHub username for cover stats (default: GiriNithin). Add GITHUB_TOKEN for higher API rate limit.
# GITHUB_USERNAME=GiriNithin
# GITHUB_TOKEN=ghp_xxxx
